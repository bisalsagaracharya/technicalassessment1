<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="employee" class="com.springcore.coninject.Employee">
        <constructor-arg value="1" />
        <constructor-arg value="Sujit" />
        <constructor-arg value="100.00" />
        <constructor-arg value="1111-11-11"/>
        <constructor-arg value="2022-11-11" />
        <constructor-arg name="address" ref="address"/>

    </bean>


    <bean id="address" class="com.springcore.coninject.Address">
        <constructor-arg value="1" type="int"/>
        <constructor-arg value="Sujit" type="String"/>
        <constructor-arg value="gg" type="String"/>

    </bean>

</beans>